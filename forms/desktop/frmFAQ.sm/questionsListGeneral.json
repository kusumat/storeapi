                    },
                    "lblQuestionTitle": {
                        "@class": "com.viz.ide.model.segment.SimpleTextSegmentData",
                        "text": "What do the hearing statuses mean?"
                    },
                    "rTextDescription": {
                        "@class": "com.viz.ide.model.segment.SimpleTextSegmentData",
                        "text": "<p><p style=\"justify; color: #000000; font-family: SourceSansPro-Regular; font-size: 18px;\"><ul style=\"padding-left:50px\"><li style=\"list-style:disc\">A “Closed” appeal means that a final decision has been made.</li><li style=\"list-style:disc\">And “Active” or “Open” appeal means that a decision is still pending.</li></ul></p></p>"
                    }
