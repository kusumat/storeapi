                    "lblQuestionTitle": {
                        "@class": "com.viz.ide.model.segment.SimpleTextSegmentData",
                        "text": "How do I change my personal information on the website?"
                    },
                    "rTextDescription": {
                        "@class": "com.viz.ide.model.segment.SimpleTextSegmentData",
                        "text": "<p tabindex=0 style=\"text-align: justify; color: #000000; font-family: SourceSansPro-Regular; font-size: 18px;\">To update any personal information in your profile, you can do so in the SHARE Portal by following the steps in the “Update Personal Demographics” section.</p>"
                    }
                },
